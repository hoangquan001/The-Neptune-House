<div class="container" style="padding: 10px 140px;">
    <div class="d-sm-flex fw-bold align-items-center justify-content-between px-sm-3">
        <div style="padding-left: 50px;">Ngày</div>
        <div>Số lượng đơn hàng</div>
        <div>Tổng thu nhập</div>
    </div>


    <% for(var i=0; i < data.length; i++) { %>
        <div class="d-sm-flex fw-bold align-items-center justify-content-between px-sm-5 py-3 my-4" style="background: #F9F9F9;
            box-shadow: 5px 7px 7px rgba(0, 0, 0, 0.25);
            ">
            <div>
                <%=(data[i].time) %>
            </div>
            <div class="pe-5">
                <%=data[i].SoLuong%>
            </div>
            <div>
                <%=data[i].ThuNhap %>đ
            </div>
        </div>
        <% } %>





</div>